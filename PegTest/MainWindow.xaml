<Window x:Class="PegTest.MainWindow" x:Name="MainMenuWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PegTest"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/blank-screen.png"/>
        </Grid.Background>
        <Label x:Name="Menu_Game_Title" Content="Peg Game" HorizontalAlignment="Left" Margin="270,23,0,0" VerticalAlignment="Top" Height="113" Width="422" FontSize="72" FontFamily="Sitka Text Semibold" FontWeight="Bold" Foreground="#FF5A1010"/>
        <Button Content="Play Game" HorizontalAlignment="Left" Margin="432,217,0,0" VerticalAlignment="Top" Height="32" Width="100" Background="#FFBDBDBD" Click="Play_Button_Click"/>
    </Grid>
</Window>
